{
    "manifest_version": 2,
    "name": "Crypto Address Finder",
    "version": "1.0",
    "description": "Find Ethereum and Bitcoin addresses on webpages.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  